package ThreadEx;

public class 스레드연습 extends ThreadEx2{

	public 스레드연습(int 초) {
		super(초);
		// TODO Auto-generated constructor stub
		System.out.println("스레드 시작");
	}

	//메인메서드 생성
	//for 문으로 스레드 5개 만들기 
	//start()로 시작 
	//for문 
	
	public static void main(String[] args) {
		
		Thread th = new Thread();
		th.start();
		for(int i = 0; i <5; i++) {
			System.out.println("스레드 종료");
				}
		
	}
	
	
	
	
}
